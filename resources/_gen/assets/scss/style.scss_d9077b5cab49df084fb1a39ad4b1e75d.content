  /* Make buttons full-width if needed for better tapping */
  .buttonPart li .btn {
    /* display: block; */
    /* width: 100%; */
    /* text-align: center; */
    /* margin-bottom: 10px; */
    /* Add space if stacked */ }
  /* You already have margin-bottom in your SCSS for buttons, which is good! */
  /* --- Navigation & Interactive --- */
  /* Dropdowns might need to be full-width or handled differently */
  .dropdown-slide:hover .dropdown-menu,
  .dropdown-slide .dropdown-menu {
    transform: none;
    /* Remove transform for simpler display */
    width: 100%;
    /* Example: make dropdowns full width */
    position: relative;
    /* Often better to make them static/relative */
    box-shadow: none;
    border: none;
    visibility: visible;
    opacity: 1;
    display: none;
    /* Rely on JS/Click to show on mobile */ }
  /* IMPORTANT: Hover-based dropdowns are not ideal for touch.
